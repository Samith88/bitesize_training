project: client-utilities
environments:
  - name: dev
    namespace: client-utilities-dev
    services:
      - name: client-utilities
        replicas: 1
        application: client-utilities
        version: 1.0.2
        limits:
          memory: 128Mi
          cpu: 100m
        deployment:
          method: rolling-upgrade
        env:
          - name: namespace
            value: client-utilities-dev
