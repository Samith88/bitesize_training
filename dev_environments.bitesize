project: lass
environments:
  - name: dev
    namespace: lass-dev
    services:
      - name: client-utilities
        replicas: 1
        application: client-utilities
        version: 1.0
        limits:
          memory: 128Mi
          cpu: 100m
        deployment:
          method: rolling-upgrade
        env:
          - name: namespace
            value: lass-dev
